{
  "name": "miningcore-webui-enhanced",
  "version": "2.0.0",
  "description": "Enhanced WebUI for Miningcore with advanced visual effects",
  "main": "index.html",
  "scripts": {
    "start": "http-server -p 8080",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss css/visual-effects.css css/animations.css -o dist/styles.min.css",
    "build:js": "uglifyjs js/miningcore.js js/visual-effects.js js/particles.js -o dist/scripts.min.js",
    "watch": "nodemon --watch css --watch js --exec npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DankMiner/Miningcore.WebUI.git"
  },
  "keywords": [
    "miningcore",
    "mining",
    "pool",
    "cryptocurrency",
    "webui",
    "dashboard",
    "visual-effects"
  ],
  "author": "DankMiner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DankMiner/Miningcore.WebUI/issues"
  },
  "homepage": "https://github.com/DankMiner/Miningcore.WebUI#readme",
  "dependencies": {
    "admin-lte": "^2.4.18",
    "bootstrap": "^3.4.1",
    "chart.js": "^2.9.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.0",
    "http-server": "^14.0.0",
    "nodemon": "^2.0.0",
    "postcss": "^8.4.0",
    "postcss-cli": "^9.0.0",
    "uglify-js": "^3.14.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
